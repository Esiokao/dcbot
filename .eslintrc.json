{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "project": "./tsconfig.json"
  },

  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": ["warn"],
    "linebreak-style": "off",
    "no-unused-vars": "warn"
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  }
}
